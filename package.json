{
  "name": "ne-home-restoration-website",
  "version": "1.0.0",
  "description": "The official website for New England Home Restoration, a home remodeling and restoration contractor based in the Boston area.",
  "main": "webpack.config.js",
  "scripts": {
    "start": "SET NODE_ENV=production webpack --config webpack.config.js && node dist/server/server.js",
    "start-dev": "npm run watch && npm run server",
    "build-dev": "SET NODE_ENV=development webpack --config webpack.config.js",
    "build-prod": "SET NODE_ENV=production webpack --config webpack.config.js",
    "server": "SET PORT=3001 && node dist/server/server.js",
    "server-dev": "SET NODE_ENV=development webpack-dev-server --watch --config webpack.config.js",
    "watch": "SET NODE_ENV=development webpack --watch --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moczka/ne-home-restoration-website"
  },
  "keywords": [
    "react",
    "boilerplate",
    "sass",
    "collocated",
    "styles"
  ],
  "author": "Javier S Guerrero",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moczka/ne-home-restoration-website/issues"
  },
  "homepage": "https://github.com/moczka/ne-home-restoration-website#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.3",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-image-gallery": "^0.8.7",
    "react-image-lightbox": "^4.6.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  }
}
